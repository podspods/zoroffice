{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/utils/*": ["utils/*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "typeRoots": [ "node_modules/@types", "i18next" ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "../__perso__/Notifications/expandDefault.ts", "../__perso__/Notifications/expandComputingNode.ts", "../__perso__/Notifications/expandMongoDb.ts", "../__perso__/Notifications/expandRabbitMQ.ts", "../__perso__/Notifications/expandRedisItem.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
